    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-hook-form": "^7.71.1",
    "react-markdown": "^10.1.0",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.13.0",
    "react-syntax-highlighter": "^16.1.0",
    "react-toastify": "^11.0.5",
    "redux-persist": "^6.0.0",
    "remark-gfm": "^4.0.1",
    "zod": "^4.3.6"
